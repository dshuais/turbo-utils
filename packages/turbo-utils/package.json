{
  "name": "turboutils",
  "version": "1.0.0",
  "description": "A library of common tools.",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "typings": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib",
    "es",
    "typings.d.ts",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./lib && rimraf ./es",
    "build:bundle": "ts-node ./build.ts",
    "build:types": "tsc -p ./tsconfig.json",
    "build": "pnpm clean && pnpm build:bundle && pnpm build:types",
    "publish:utils": "npm publish --registry https://registry.npmjs.org"
  },
  "keywords": [
    "utils",
    "turbo",
    "turbo-utils",
    "common",
    "hooks",
    "tools"
  ],
  "author": "dshuais",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dshuais/turbo-utils.git"
  },
  "homepage": "https://dshuais.netlify.app/",
  "dependencies": {
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.5"
  }
}
